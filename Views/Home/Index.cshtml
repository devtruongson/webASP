@{
    ViewData["Title"] = "Xe Máy Trang Chủ";
    ViewData["Description"] = "MOTOGO là dịch vụ cho thuê xe máy tự lái chuyên nghiệp tại Việt Nam, cho phép người thuê có thể nhận xe tại một điểm và trả xe tại một điểm khác trên cùng một hệ thống.";
}

<div>
    <partial name="~/Views/Shared/Components/home/Form/form.cshtml" />
    <partial name="~/Views/Shared/Components/home/Destination/destination.cshtml" />
    <partial name="~/Views/Shared/Components/home/CateCars/CateCars.cshtml" />
    <partial name="~/Views/Shared/Components/home/Detail/Detail.cshtml" />
    <partial name="~/Views/Shared/Components/home/Procedure/Procedure.cshtml" />
  @{
    var dataList = ViewBag.Data as List<string>;
}

@if (dataList != null)
{
    <ul>
        @foreach (var item in dataList)
        {
            <li class="Data_list">@item</li>
        }
    </ul>
}
else
{
    <p>No data available</p>
}
</div>
